		"@types/expect": "^24.3.0",
		"@types/mocha": "^8.2.2",
		"@types/node": "^15.0.2",
		"@types/wav": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"chai": "^4.3.4",
		"chai-as-promised": "^7.1.1",
		"eslint": "^7.26.0",
		"eslint-config-google": "^0.14.0",
		"mocha": "^8.4.0",
		"nodemon": "^2.0.7",
		"nyc": "^15.1.0",
		"source-map-support": "^0.5.19",
		"ts-mocha": "^8.0.0",
		"ts-node": "^9.1.1",
		"typedoc": "^0.20.36",
