	"author": "Rei666",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Rei-666/discord-speech-recognition/issues"
	},
	"homepage": "https://discordsr.netlify.app/",
	"engines": {
		"node": ">=12.0.0"
	},
	"dependencies": {
		"@discordjs/opus": "^0.3.3",
		"axios": "^0.21.1",
		"discord.js": "^12.5.3",
		"node-witai-speech": "^1.0.2",
		"request": "^2.88.2",
		"underscore": "^1.13.1",
		"wav": "^1.0.2"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.1",
